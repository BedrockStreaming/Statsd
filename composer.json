{
    "name": "m6web/statsd",
    "description" : "Component easing the statsd usage",
    "type": "library",
    "license": "MIT",
    "keywords": ["php", "statsd", "m6web"],
    "config": {
        "vendor-dir": "vendor"
    },
    "minimum-stability" : "dev",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "atoum/atoum": "^2.8|^3.0",
        "m6web/php-cs-fixer-config": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "M6Web\\Component\\Statsd\\": "src/M6Web/Component/Statsd"
        },
        "exclude-from-classmap": [
            "src/M6Web/Component/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "M6Web\\Component\\Statsd\\Tests\\": "src/M6Web/Component/Tests/"
        }
    }
}
